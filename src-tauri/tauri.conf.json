{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Telegram File Server",
  "version": "0.1.0",
  "identifier": "com.telegram.fileserver",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:8080",
    "beforeBuildCommand": "",
    "frontendDist": "../Frontend/dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Telegram File Server",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ],
    "resources": [
      "./frontend-dist/**/*"
    ],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "deb": {
      "depends": []
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "http": {
      "permissions": [
        "allow-fetch",
        "allow-cookie"
      ]
    }
  }
}