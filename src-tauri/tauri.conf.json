{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Telegram File Server",
  "version": "0.1.0",
  "identifier": "com.telegram.fileserver",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "",
    "frontendDist": "../src/Frontend/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Telegram File Server",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "http": {
      "permissions": [
        "allow-fetch",
        "allow-cookie"
      ],
      "defaultHeaders": {
        "withCredentials": true
      }
    }
  }
}